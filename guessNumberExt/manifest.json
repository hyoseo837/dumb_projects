{
    "name" : "guessing number extension",
    "description" : "this extension is for studying JS",
    "version" : "1.0",
    "manifest_version" : 3,
    "background" : {
        "service_worker" : "background.js"
    },
    
    "permissions": ["storage"],

    "action" : {
        "default_popup" : "popup.html",
        "default_icons" : {
            "16" : "/images/guessing_icon_16.png",
            "32" : "/images/guessing_icon_32.png",
            "64" : "/images/guessing_icon_64.png",
            "128" : "/images/guessing_icon_128.png"
            }
    },
    "icons" : {
        "16" : "/images/guessing_icon_16.png",
        "32" : "/images/guessing_icon_32.png",
        "64" : "/images/guessing_icon_64.png",
        "128" : "/images/guessing_icon_128.png"
    }
}